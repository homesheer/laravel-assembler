{
    "name": "homesheer/laravel-assembler",
    "description": "eloquent model or DTO assembler for Laravel",
    "type": "library",
    "keywords": [
        "model",
        "DTO",
        "assembler"
    ],
    "homepage": "http://i-mars.com/laravel-assembler",
    "license": "MIT",
    "authors": [
        {
            "name": "homesheer",
            "email": "homesheer@qq.com",
            "homepage": "http://homesheer.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "HomeSheer\\LaravelAssembler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HomeSheer\\LaravelAssembler\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "HomeSheer\\LaravelAssembler\\AssemblerServiceProvider"
            ]
        }
    }
}
